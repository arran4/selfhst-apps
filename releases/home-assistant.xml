  <entry>
    <id>https://github.com/home-assistant/core/releases/tag/2025.5.2</id>
    <title>New release for Home Assistant: 2025.5.2</title>
    <updated>2025-05-16T17:09:45-04:00</updated>
    <author>
      <name>home-assistant/core</name>
    </author>
